{
  "name": "whats-vantage",
  "description": "Whats Vantage is a Chrome extension that helps you manage your WhatsApp conversations.",
  "license": "Apache-2.0",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:dev": "wxt build --mv3 --mode=development",
    "build:dev:firefox": "wxt build -b firefox --mv3 --mode=development",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "lint": "eslint  --fix .",
    "init:wa-js": "node ./scripts/init-wa-js.js",
    "init:fix-husky": "node ./scripts/fix-husky.js",
    "postinstall": "pnpm init:wa-js && wxt prepare",
    "prepare": "pnpm init:fix-husky && husky"
  },
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@emotion/cache": "^11.14.0",
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@jvitela/mustache-wax": "^1.0.4",
    "@radix-ui/colors": "^3.0.0",
    "@radix-ui/react-menubar": "^1.1.14",
    "@tanstack/react-query": "^5.75.7",
    "@tanstack/react-query-devtools": "^5.75.7",
    "@webext-core/messaging": "^2.2.0",
    "@wppconnect/wa-js": "^3.17.2",
    "@wxt-dev/i18n": "^0.2.4",
    "ahooks": "^3.8.4",
    "antd": "^5.25.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dexie": "^4.0.11",
    "dexie-observable": "4.0.1-beta.13",
    "extend-excel": "^0.0.6",
    "file-type": "^20.5.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.509.0",
    "mustache": "^4.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-transition-state": "^2.3.1",
    "react-use-event-hook": "^0.9.6",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.26.0",
    "@swc/plugin-emotion": "^9.0.4",
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@types/chrome": "^0.0.321",
    "@types/debug": "^4.1.12",
    "@types/lodash-es": "^4.17.12",
    "@types/mustache": "^4.2.6",
    "@types/react": "^19.1.3",
    "@types/react-devtools": "^3.6.2",
    "@types/react-dom": "^19.1.3",
    "@typescript-eslint/parser": "^8.32.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "chalk": "^5.4.1",
    "debug": "^4.4.0",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import-x": "^4.11.1",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^15.5.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vite-plugin-javascript-obfuscator": "^3.1.0",
    "wxt": "^0.20.6"
  },
  "homepage": "https://github.com/gideonstele/whats-vantage",
  "bugs": {
    "url": "https://github.com/gideonstele/whats-vantage/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gideonstele/whats-vantage.git"
  },
  "packageManager": "pnpm@10.10.0",
  "volta": {
    "node": "22.15.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "^18.3.1 || ^19.0.0"
      }
    },
    "onlyBuiltDependencies": [
      "@swc/core",
      "dtrace-provider",
      "esbuild",
      "javascript-obfuscator",
      "spawn-sync",
      "unrs-resolver"
    ]
  }
}
